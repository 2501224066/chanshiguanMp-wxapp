.navbar {
  width: 100%;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-items: center;
  z-index: 99999;
  position:fixed;
  left: 0;
  top: 0;
  z-index: 99999;
}

.content {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.content>.title {
  height: 100%;
  display: flex;
  justify-content: space-between;
}

.content>.title>.edge {
  width: 160rpx;
}

.content>.title>.edge>.capsule {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  height: 100%;
  border-radius: 80rpx;
  border: 1rpx solid #ececec;
}

.content>.title>.edge>.capsule>.img {
  width: 33rpx;
  height: 33rpx;
  line-height: 0;
}

.content>.title>.edge>.capsule>.erect {
  width: 2rpx;
  height: 80%;
  border-right: 1rpx solid #ececec;
}

.content>.title>.text {
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.content>.title>.text>.title {
  font-weight: 900;
}

.content>.title>.text>.pid {
  margin-top: 4rpx;
  color: #97989B;
  font-size: 21rpx;
}

.content>.region {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
}

.content>.region>.img {
  width: 26.6rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.content>.region>.img2 {
  width: 18rpx;
  height: 10rpx;
  line-height: 0;
  margin-left: 14rpx;
}

.dump {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-weight: 900;
}

.dump>.img {
  width: 52rpx;
  height: 52rpx;
  margin-right: 20rpx;
}